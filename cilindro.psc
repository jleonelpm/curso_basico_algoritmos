Proceso cilindro
	
	Definir radio Como Real;
	Definir altura Como Real;
	DEfinir v Como Real;
	Definir area Como Real;
	
	EScribir "Ingresa el radio";
	leer radio;
	Escribir "ingresa la altura";
	leer altura;
	
	v = 3.1416 * (radio^2)*altura;
	area = 2 * 3.1416 * radio * altura;
	
	escribir "El resultado del volumen es ", v;
	Escribir "El resultado del area es ", area;
	
	
FinProceso
